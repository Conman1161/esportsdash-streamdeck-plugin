{
	"extends": "@tsconfig/node20/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"customConditions": [
			"node",
		],
		"module": "ES2022",
		"moduleResolution": "Bundler",
		"noImplicitOverride": true
	},
	"include": [
		"src/**/*.ts",
		"src/websocket/socket.ts"
	],
	"exclude": [
		"node_modules"
	]
}